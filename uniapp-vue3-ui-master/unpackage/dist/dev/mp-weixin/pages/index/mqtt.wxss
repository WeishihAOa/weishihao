
.input-field {
		display: inline-block;
		width: 200px;
		margin-right: 10px;
		border: 2px solid skyblue;
		/* 增加边框宽度 */
		background-color: #f0f0f0;
		/* 设置背景色 */
		color: #333;
		/* 设置文字颜色 */
		padding: 10px;
		/* 调整内边距 */
		border-radius: 5px;
		/* 添加圆角 */
}
.view {
		line-height: 10px;
}
.btn {
		display: flex;
		width: 100%;
		white-space: break-spaces;
}
